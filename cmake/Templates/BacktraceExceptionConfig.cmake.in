@PACKAGE_INIT@

#Include targets file.  This will create IMPORTED target @PROJECT_NAME@
include("${CMAKE_CURRENT_LIST_DIR}/@EXPORT_TARGETS_NAME@.cmake")

#Single-library package with epynomous library
#Sets:
# @PROJECT_NAME@_LIBRARIES
# @PROJECT_NAME@_SHARED_LIBRARIES
# @PROJECT_NAME@_STATIC_LIBRARIES

if(@BUILD_SHARED_LIBS@)
    set(@PROJECT_NAME@_SHARED_LIBRARIES @PROJECT_NAME@)
    message(STATUS "SHARED_LIBRARIES ${@PROJECT_NAME@_SHARED_LIBRARIES}")
else()
    set(@PROJECT_NAME@_SHARED_LIBRARIES)
endif()
if(@BUILD_STATIC_LIBS@)
    set(@PROJECT_NAME@_STATIC_LIBRARIES @PROJECT_NAME@_static)
    message(STATUS "STATIC_LIBRARIES ${@PROJECT_NAME@_STATIC_LIBRARIES}")
else()
    set(@PROJECT_NAME@_STATIC_LIBRARIES)
endif()

if(@BUILD_SHARED_LIBS@)
    set(@PROJECT_NAME@_LIBRARIES ${@PROJECT_NAME@_SHARED_LIBRARIES})
elseif(@BUILD_STATIC_LIBS@)
    set(@PROJECT_NAME@_LIBRARIES ${@PROJECT_NAME@_STATIC_LIBRARIES})
endif()
message(STATUS "LIBRARIES ${@PROJECT_NAME@_LIBRARIES}")

get_property(@PROJECT_NAME@_BUILD_TYPES TARGET ${@PROJECT_NAME@_LIBRARIES} PROPERTY IMPORTED_CONFIGURATIONS)
